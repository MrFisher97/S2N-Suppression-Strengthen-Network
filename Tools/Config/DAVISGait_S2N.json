{
    "Data": {
        "dataset": "DAVISGait",
        "root": "Dataset/DAVISGait/",
        "data_file": "C36W03.h5",
        "map_file": "map.csv",
        "scene": "l16",
        "__scene": ["l0", "l4", "l16", "l64", "all"],
        "num_samples": null, 
